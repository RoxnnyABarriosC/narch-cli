{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "no-trailing-whitespace": false,
    "trailing-comma": [ false ],
    "no-console": false,
    "prefer-const": false,
    "no-empty-interface": false
  },
  "linterOptions": {
    "exclude": [
      "src/templates/**/files/*.ts"
    ]
  }
}